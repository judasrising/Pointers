#include<stdio.h>

int main()
{
    int num1,num2;
    printf("Enter first number: ");scanf("%d",&num1);
    printf("Enter second number: ");scanf("%d",&num2);

    printf("Numbers before swap:\n%d %d",num1,num2);

    swap(&num1,&num2);

    printf("\nNumbers after swap:\n%d %d",num1,num2);
}

void swap(int *p,int *q)
{
    int temp;
    temp=*q;
    *q=*p;
    *p=temp;
}
